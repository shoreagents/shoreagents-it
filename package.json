{
  "name": "shoreagents-ai",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"node server.js\" \"wait-on http://localhost:3001 && electron .\"",
    "dev:web": "node server.js",
    "dev:electron": "wait-on http://localhost:3001 && electron .",
    "build": "next build",
    "start": "node server.js",
    "lint": "next lint",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3001 && electron .\"",
    "electron-pack": "npm run build && electron-builder",
    "electron-build": "next build && next export && electron-builder",
    "setup-db": "node scripts/setup-db.js",
    "add-position": "node scripts/add-position-column.js",
    "apply-realtime": "node scripts/apply-realtime-schema.js",
    "test-db": "node scripts/test-db.js",
    "test-bpoc-db": "node scripts/test-bpoc-db.js",
    "test-applicant-update": "node scripts/test-applicant-update.js",
    "test-redis": "node scripts/test-redis.js",
    "check-redis": "node scripts/check-redis-data.js",
    "create-accounts": "node scripts/create-supabase-accounts.js",
    "apply-member-triggers": "psql $DATABASE_URL -f scripts/create-member-triggers.sql",
    "test-member-realtime": "node scripts/test-member-realtime.js",
    "apply-members-activity-log": "psql $DATABASE_URL -f scripts/apply-members-activity-log.sql",
    "apply-members-activity-log-simple": "psql $DATABASE_URL -f scripts/apply-members-activity-log-simple.sql",
    "apply-members-activity-log-fields": "psql $DATABASE_URL -f scripts/apply-members-activity-log-fields.sql"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@number-flow/react": "^0.5.10",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/supabase-js": "^2.53.0",
    "@tabler/icons-react": "^3.34.1",
    "@tanstack/react-table": "^8.21.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/ws": "^8.18.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "lucide-react": "^0.528.0",
    "motion": "^12.23.12",
    "next": "15.4.4",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-color": "^2.19.3",
    "react-day-picker": "^9.8.1",
    "react-dom": "19.1.0",
    "recharts": "^2.15.4",
    "redis": "^5.8.2",
    "shadcn-ui": "^0.9.5",
    "sonner": "^2.0.6",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.1",
    "vaul": "^1.1.2",
    "ws": "^8.18.3",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "electron": "^37.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "wait-on": "^8.0.4"
  },
  "build": {
    "appId": "com.shoreagents.ai",
    "productName": "ShoreAgents AI",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      "out/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
